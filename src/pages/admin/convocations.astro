---
import React from 'react'
import Layout from "../../layouts/Layout.astro";
import NavBar from "../../layouts/NavBar.astro";
import AdminConvocations from "../../components/AdminConvocations";

const config = {
    apiUrl: import.meta.env.API_URL,
    appTitle: import.meta.env.PUBLIC_APP_TITLE
}

---

<Layout title={config.appTitle}>
        <NavBar title={config.appTitle}></NavBar>  
        <React.StrictMode>
            <AdminConvocations apiUrl={config.apiUrl} client:load/>
        </React.StrictMode>
</Layout>