---
import Layout from "../layouts/Layout.astro";
import NavBar from "../layouts/NavBar.astro";
import TestsTable from "../components/testsTable";

const config = {
    apiUrl: import.meta.env.API_URL,
    appTitle: import.meta.env.PUBLIC_APP_TITLE
}
---

<Layout title={import.meta.env.PUBLIC_APP_TITLE}>
    <NavBar title={import.meta.env.PUBLIC_APP_TITLE} />
    <TestsTable apiUrl={config.apiUrl} adminPage={false} client:load></TestsTable>
</Layout>
